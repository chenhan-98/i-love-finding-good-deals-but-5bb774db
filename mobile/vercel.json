{
  "buildCommand": "NODE_ENV=production EAS_NO_VCS=1 EXPO_NO_SOURCE_MAP=1 npx expo export -p web --no-minify --no-bytecode",
  "outputDirectory": "dist",
  "devCommand": "npx expo start --web",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "ALLOWALL"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors *"
        }
      ]
    }
  ]
}